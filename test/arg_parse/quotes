#!/usr/bin/env bash

source ../.defaults

$BINARY > minishell.out <<EOF 2> /dev/null
echo "Stuff happens here"
echo harder" stuff and things"
echo "other stuff and things"  
echo " things that might not seem "hard
echo " and legitimately"hard seeming stuff   
EOF

MSH_RET=$?

diff -u .quotes.diff minishell.out

RET=$?

rm minishell.out

if [ $MSH_RET != 0 ]
then
    exit 1
fi

exit $RET

# Local Variables:
# mode: shell-script
# prelude-clean-whitespace-on-save: nil
# End:
